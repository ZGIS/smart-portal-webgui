    "webpack-validator": "^2.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ZGIS/smart-portal-webgui.git"
  },
  "bugs": {
    "url": "https://github.com/ZGIS/smart-portal-webgui/issues"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  }
}
