{
  "name": "smart-portal-webgui",
  "version": "1.0.0",
  "author": "SAC",
  "description": "SMART Portal Web Frontend",
  "main": "src/app/main.ts",
  "keywords": [
    "angular2",
    "openlayers",
    "bootstrap",
    "groundwater",
    "geospatial",
    "webpack",
    "typescript"
  ],
  "contributors": [
    "Alexander Kmoch  <allixender@gmail.com>",
    "Steffen R <grmpfhmbl@googlemail.com>"
  ],
  "homepage": "https://github.com/ZGIS/smart-portal-webgui",
  "license": "Apache-2.0",
  "scripts": {
    "start:dev-server": "webpack-dev-server --inline --progress --port 8080",
    "start": "npm run start:dev-server",
    "start:dll": "run-s build:dll start:dev-server",
    "pretest": "npm run clean:coverage",
    "test": "karma start",
    "test:ci": "npm test -- --ci",
    "prebuild": "npm run clean:build",
    "serve:build": "run-s build:jit serve",
    "build:sphinx-docs": "sphinx-build -b html src/docs/source sphinx-build",
    "build:dll": "webpack --config config/webpack/webpack.dll.js --progress --profile --bail --display-error-details",
    "build:jit": "webpack --config config/webpack/webpack.prod-jit.js --progress --profile --bail --display-error-details",
    "build": "rimraf dist && webpack --config config/webpack/webpack.prod.js --progress --profile --bail",
    "clean": "run-p clean:*",
    "clean:build": "rimraf dist",
    "clean:aot": "rimraf aot",
    "clean:coverage": "rimraf coverage",
    "clean:dll": "rimraf dll-dist",
    "clean:sphinx-docs": "rimraf sphinx-docs",
    "tslint": "tslint -t verbose './src/**/*.ts'",
    "test:single": "karma start karma.conf.js --single-run",
    "test:watch": "karma start",
    "e2e": "cypress run",
    "cypress": "cypress",
    "cypress:demo": "cypress run --config chromeWebSecurity=false --spec cypress/integration/gwhub_cy_spec.js",
    "e2e:watch": "cypress open",
    "e2e:ci": "npm run build && concurrently --kill-others --success first \"http-server -p 8080 ./dist\" \"npm run e2e\"",
    "e2e:ci:jit": "npm run build:jit && concurrently --kill-others --success first \"http-server -p 8080 ./dist\" \"npm run e2e\"",
    "gh-pages-push": "gh-pages -d ghdash -r git@github.com:ZGIS/smart-portal-webgui.git",
    "serve": "http-server -p 8080 ./dist",
    "serve-cors-9000": "http-server -p 8080 ./dist -o -P http://localhost:9000 --cors",
    "stat": "node ./config/dashboard"
  },
  "dependencies": {
    "@angular/common": "~4.4.6",
