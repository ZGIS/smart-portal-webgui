{
  "name": "smart-portal-webgui",
  "version": "1.0.0",
  "author": "SAC",
  "description": "SMART Portal Web Frontend",
  "main": "src/app/main.ts",
  "keywords": [
    "angular2",
    "webpack",
    "typescript"
  ],
  "contributors": [
    "Steffen  <grmpfhmbl@googlemail.com>",
    "Alex  <allixender@googlemail.com>"
  ],
  "homepage": "https://github.com/ZGIS/smart-portal-webgui",
  "license": "Apache-2.0",
  "scripts": {
    "start:dev-server": "webpack-dev-server --inline --progress --port 8080",
    "start": "webpack-dashboard -- webpack-dev-server --inline --port 8080",
    "start:dll": "run-s build:dll start",
    "pretest": "npm run clean:coverage",
    "test": "karma start",
    "test:ci": "npm test -- --ci",
    "prebuild": "npm run clean:build",
    "serve:build": "run-s build serve",
    "build:dll": "webpack --config config/webpack/webpack.dll.js --progress --profile --bail",
    "build:jit": "webpack --config config/webpack/webpack.prod-jit.js --progress --profile --bail",
    "build": "rimraf dist && webpack --config config/webpack/webpack.prod.js --progress --profile --bail",
    "clean": "run-p clean:*",
    "clean:build": "rimraf dist",
    "clean:coverage": "rimraf coverage",
    "clean:dll": "rimraf dll-dist",
    "tslint": "tslint ./src/**/*.ts -t verbose",
    "test:single": "karma start karma.conf.js --single-run",
    "test:watch": "karma start",
    "pree2e": "npm run webdriver:update -- --standalone",
    "e2e": "protractor",
    "e2e:watch": "npm run e2e -- --elementExplorer",
    "e2e:ci": "npm run build && concurrently --kill-others --success first \"http-server -p 8080 ./dist\" \"npm run e2e\"",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "npm run webdriver-manager update",
    "gh-pages-push": "gh-pages -d dist -r git@github.com:ZGIS/smart-portal-webgui.git",
    "server": "http-server -p 8080 ./dist",
    "stat": "node ./config/dashboard"
  },
