{
  "name": "smart-portal-webgui",
  "version": "1.0.0",
  "description": "SMART Portal Web Frontend",
  "keywords": [
    "angular2",
    "webpack",
    "typescript"
  ],
  "contributors": [
    "Steffen  <grmpfhmbl@googlemail.com>",
    "Alex  <allixender@googlemail.com>"
  ],
  "homepage": "https://github.com/ZGIS/smart-portal-webgui",
  "license": "Apache-2.0",
  "scripts": {
    "start": "webpack-dev-server --inline --progress --port 8080",
    "clean": "npm cache clean && rimraf -- dist coverage",
    "test": "karma start karma.conf.js --single-run",
    "test:watch": "karma start",
    "e2e": "protractor",
    "e2e:watch": "npm run e2e -- --elementExplorer",
    "e2e:ci": "npm run build && concurrently --kill-others --success first \"http-server -p 8080 ./dist\" \"npm run e2e\"",
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "gh-pages-push": "gh-pages -d dist -r git@github.com:ZGIS/smart-portal-webgui.git",
    "tslint": "tslint ./src/app/**/*.ts -t verbose",
    "typedoc": "typedoc",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "webpack-validate:dev": "webpack-validator config/webpack.dev.js",
    "webpack-validate:test": "webpack-validator config/webpack.test.js",
    "webpack-validate:prod": "webpack-validator config/webpack.prod.js",
    "webdriver:start": "webdriver-manager start",
    "webdriver:update": "webdriver-manager update"
  },
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "@angular/upgrade": "2.0.0",
    "angular-in-memory-web-api": "0.1.1",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "font-awesome": "~4.6.3",
    "ie-shim": "^0.1.0",
    "ng2-bootstrap": "1.1.5",
    "moment": "^2.15.0",
    "openlayers": "^3.18.2",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.7",
    "@types/jasmine": "^2.2.34",
    "@types/node": "^6.0.38",
    "@types/openlayers": "^3.6.36",
    "@types/selenium-webdriver": "2.53.30",
    "@types/source-map": "^0.1.27",
    "@types/uglify-js": "^2.0.27",
    "@types/webpack": "^1.12.34",
    "angular2-template-loader": "^0.5.0",
    "assets-webpack-plugin": "^3.4.0",
    "awesome-typescript-loader": "^2.2.4",
    "canonical-path": "0.0.2",
    "codelyzer": "~0.0.28",
    "concurrently": "^3.1.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.25.0",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "gh-pages": "^0.11.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.21.0",
    "http-server": "^0.9.0",
    "imports-loader": "^0.6.5",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "^2.7.0",
    "json-loader": "^0.5.4",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0 ",
    "karma-cli": "^0.1.2",
    "karma-coverage": "^1.1.1",
    "karma-htmlfile-reporter": "^0.2.2",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-remap-coverage": "^0.1.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "1.8.0",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "protractor": "^4.0.9",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "selenium-webdriver": "~2.53.3",
    "source-map-loader": "^0.1.5",
    "string-replace-loader": "1.0.5",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.4",
    "ts-helpers": "1.1.1",
    "tslint": "3.15.1",
    "tslint-loader": "^2.1.3",
    "typedoc": "^0.4.5",
    "typescript": "^2.0.3",
    "typings": "^1.3.2",
